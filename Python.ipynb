    }
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "a is None: True\n",
      "a == None: True\n"
     ]
    }
   ],
   "source": [
    "# Both will return True\n",
    "print(\"a is None: {}\".format(a is None))\n",
    "print(\"a == None: {}\".format(a == None))"
   ]
  }
